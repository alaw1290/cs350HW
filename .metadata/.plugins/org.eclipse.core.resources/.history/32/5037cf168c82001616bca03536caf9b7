package hw2;

public class problem4 {
	
	public static double expdist(int lambda){
		double result = -1*Math.log(1-Math.random() / lambda);
		return result;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i = 0; i < 10; i++){
			System.out.println(expdist(3));
		};
	}
}
