package hw3;

public class Logger {
	private double totalTW;
	private double totalTS;
	private double totalRequests;
	private int totalW;
	private int totalQ;
	private int totalP;
	private int totalLogs;
	
	public Logger(){
		totalTW = 0;
		totalTS = 0;
		totalRequests = 0;
		totalW = 0;
		totalQ = 0;
		totalP = 0;
		totalLogs = 0;
	}
	
	public void writeLog(int w, int q, int p){
		totalLogs++;
		
	}
}
