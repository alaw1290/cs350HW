package hw6;

public class WhileTimer {
	
	public static void main(String[] args) {
		
		int c = 0;
		long start = System.currentTimeMillis();
		while(c < 251){
			c++;
			System.out.println(c);
		}
		long end = System.currentTimeMillis();
		long time = end-start;
		System.out.println(time);

	}

}
