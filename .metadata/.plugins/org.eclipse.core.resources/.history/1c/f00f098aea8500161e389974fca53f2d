package hw3;

public class MM1Event {
	private double lambda;
	private double tService;
	private double currentTime;
	private double loggingTime;
	
	public MM1Event(double lamb, double ts, double tc, double tl){
		lambda = lamb;
		tService = ts;
		currentTime = tc;
	}
	
	private double expDist(int lambda){
		// Input lambda (interarrival time) to return exponentially 
		// distributed values
		double result = -1*Math.log(1-Math.random()) / lambda;
		return result;
	}
	
