package hw4;

import java.util.*;

public class Logger {
	private LinkedList<Double> totalSysTQ;
	private LinkedList<Double> totalCPUQ;
	private double pCPU;
	private double pNet;
	private double pDisk;
	private double totalLogs;
	
	
	
	public Logger(){
		totalSysTQ = new LinkedList<Double>();
		totalCPUQ = new LinkedList<Double>();
		pCPU = 0;
		pNet = 0;
		pDisk = 0;
		totalLogs = 0;
	}
	
	public void writeLog(double qCPU, int CPU, int Net, int Disk){
		totalLogs+= 1;
		totalCPUQ.add(qCPU);
		pCPU += CPU;
		pNet += Net;
		pDisk += Disk;
	}
	
	public void logRequest(double tq){
		totalSysTQ.add(tq);
	}
}
